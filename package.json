{
  "name": "n-comp",
  "description": "CLI for creating new react components",
  "version": "0.1.6",
  "keywords": [
    "react",
    "reactjs",
    "cli",
    "component",
    "boilerplate"
  ],
  "author": {
    "name": "Alexis Guzman"
  },
  "homepage": "https://github.com/codingcodax/new-comp#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/codingcodax/new-comp"
  },
  "bugs": {
    "url": "https://github.com/codingcodax/new-comp/issues"
  },
  "license": "MIT",
  "bin": {
    "n-comp": "./lib/src/index.js"
  },
  "files": [
    "lib"
  ],
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "lint": "eslint src/**",
    "lint:fix": "eslint src/** --fix",
    "format": "prettier --write .",
    "prebuild": "rm -rf lib/",
    "build": "tsc && cp -r src/templates/ lib/src/"
  },
  "devDependencies": {
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "prettier": "^3.0.0"
  }
}
